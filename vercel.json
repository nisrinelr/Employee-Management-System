{
    "builds": [{
        "src":"employee_manager/wsgi.py",
        "use":"employee_manager/python",
        "config":{"maxLambdaSize":"15mb", "runtime":"python3.12"}
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "employee_manager/static"
        },
        {
            "src": "/(.*)",
            "dest": "employee_manager/wsgi.py"
        }
    ]
}